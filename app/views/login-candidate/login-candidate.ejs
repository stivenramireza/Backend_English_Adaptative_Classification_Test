    <option value="7">Documento Nacional de Identidad</option>
    <option value="8">Documento Personal Identificación</option>
    <option value="9">Nro Único Identificación Personal</option>
    <option value="10">Otro</option>
    <option value="11">Pasaporte</option>
    <option value="12">Registro</option>
    <option value="13">Tarjeta de Identidad</option>
  </select>
  <br/>
  <input id="docnumber" type="identification" class="form-control" placeholder="Número de Identificación" required autofocus>
  <div class="checkbox mb-3">
  </div>
  <br>
  <button id="btnLogin"class="btn btn-lg btn-primary btn-block" onclick=login() type="button">Ingresar</button>
