            <hr class="sidebar-divider">

            <div class="sidebar-heading">
                Examen
            </div>

            <li class="nav-item active">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#preguntas"
                    aria-expanded="true" aria-controls="preguntas">
                    <i class="fas fa-question"></i>
                    <span>Preguntas</span>
                </a>
                <div id="preguntas" class="collapse" aria-labelledby="headingPages" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <h6 class="collapse-header">Gestón de preguntas:</h6>
                        <a class="collapse-item"
                            href="/admin/profile/add-question"><b>Agregar
                            pregunta nueva</b></a>
                        <a class="collapse-item"
                            href="/admin/profile/edit-question">Editar
                            pregunta</a>
                    </div>
                </div>
            </li>

            <hr class="sidebar-divider d-none d-md-block">

        </ul>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <div class="container" style="max-width: 700px">
                        <div class="py-5 text-center">
                            <p class="lead">Complete el siguiente formulario para agregar una nueva pregunta a la base
                                de datos</p>
                        </div>
                        <div class="container" style="width: 100%">
                            <form class="needs-validation" novalidate>
                                <div class="row">
                                    <div class="col mb-3">
                                        <label for="firstName">Pregunta</label>
                                        <textarea name="textarea" style="width:600px;height:50px;" id="question" placeholder="" value=""
                                        required></textarea>
                                        <div class="invalid-feedback">
                                            Se requiere que llene el campo "Pregunta" correctamente.
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-10 mb-4">
                                        <label for="firstName">Respuesta 1</label>
                                        <input type="text" class="form-control" id="answer1" placeholder="" value=""
                                            required>
                                        <div class="invalid-feedback">
                                            Se requiere que llene el campo "Respuesta 1" correctamente.
                                        </div>
                                    </div>
                                    <div class="col-md-2 mb-4">
                                        <br>
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" onclick="uncheck()" name="answer"
                                                id="answer1c">
                                            <label class="custom-control-label" for="answer1c">Correcta</label>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-10 mb-3">
                                        <label for="firstName">Respuesta 2</label>
                                        <input type="text" class="form-control" id="answer2" placeholder="" value=""
                                            required>
                                        <div class="invalid-feedback">
                                            Se requiere que llene el campo "Respuesta 2" correctamente.
                                        </div>
                                    </div>
                                    <div class="col-md-2 mb-3">
                                        <br>
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" onclick="uncheck()" name="answer"
                                                id="answer2c">
                                            <label class="custom-control-label" for="answer2c">Correcta</label>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-10 mb-3">
                                        <label for="firstName">Respuesta 3</label>
                                        <input type="text" class="form-control" id="answer3" placeholder="" value=""
                                            required>
                                        <div class="invalid-feedback">
                                            Se requiere que llene el campo "Respuesta 3" correctamente.
                                        </div>
                                    </div>
                                    <div class="col-md-2 mb-3">
                                        <br>
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" onclick="uncheck()" name="answer"
                                                id="answer3c">
                                            <label class="custom-control-label" for="answer3c">Correcta</label>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-5 mb-3">
                                        <label for="state">Dificultad de la pregunta</label>
                                        <select class="custom-select d-block w-100" id="difficult" required>
                                            <option value="0">Facil</option>
                                            <option value="1">Intermedia</option>
                                            <option value="2">Avanzada</option>
                                        </select>
                                        <div class="invalid-feedback">
                                            Por favor seleccione una dificultad.
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <br>


                        <hr class="mb-4" style="width: 100%">

                        <center>
                            <button class="btn btn-primary" onclick=signup() type="button">Agregar pregunta</button>
                        </center>
                        </form>
                        <br>
                    </div>
                </div>
                <br>
            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span> &copy; 2019 Universidad EAFIT</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
