        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <div class="container" style="max-width: 700px">
                        <div class="py-5 text-center">
                            <p class="lead">Señor Administrador, ingrese la identificacion del administrador que desea actualizar.</p>
                        </div>
                        <div class="container" style="width: 100%">
                            <form class="needs-validation" novalidate>
                                <div class="row">
                                    <div class="col mb-3">
                                        <label for="country">Tipo de documento</label>
                                        <select class="custom-select d-block w-100" id="dt" required>
                                            <option value="0">Seleccione el tipo de identificación...</option>
                                            <option value="1">Cédula de Ciudadanía</option>
                                            <option value="2">Cédula de Ciudadanía Ecuatoriana</option>
                                            <option value="3">Cédula de Extranjería</option>
                                            <option value="4">Cédula de Guatemala</option>
                                            <option value="5">Cédula de Panamá</option>
                                            <option value="6">Código de Estudiante</option>
                                            <option value="7">Documento Nacional de Identidad</option>
                                            <option value="8">Documento Personal Identificación</option>
                                            <option value="9">Nro Único Identificación Personal</option>
                                            <option value="10">Otro</option>
                                            <option value="11">Pasaporte</option>
                                            <option value="12">Registro</option>
                                            <option value="13">Tarjeta de Identidad</option>
                                        </select>
                                        <div class="invalid-feedback">
                                            Por favor seleccione un tipo de documento.
                                        </div>
                                    </div>

                                    <div class="col mb-3">
                                        <label for="firstName">Documento de identidad</label>
                                        <input type="text" class="form-control" id="docnumber" placeholder="" value=""
                                            required>
                                        <div class="invalid-feedback">
                                            Se requiere que llene el campo "Documento de identidad" correctamente.
                                        </div>
                                    </div>
                                </div>

                                <center>
                                    <button class="btn btn-primary" onclick=gestionar() type="button">Gestionar</button>
                                </center>
                            </form>
                            <br>
                        </div>
                    </div>
                    <br>
                </div>

                <!-- End of Main Content -->

                <!-- Footer -->
                <center>
                    <footer class="sticky-footer bg-white" style="position: absolute; bottom: 0; width: 83%">
